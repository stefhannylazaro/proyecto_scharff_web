button.btn-menu.btn
  &:hover
    background-color: none!important
  .btn__content:before
    border-radius none
    color c-text-title-pink
    content ""
    position absolute
    left 35%
    top 43px
    height .3rem;
    opacity 1
    -webkit-transition: .3s cubic-bezier(.25,.8,.5,1);
    transition: .3s cubic-bezier(.25,.8,.5,1);
    width: 30%;


.toolbar__items:hover .parentOp-dom.btn-menu.btn .btn__content:before
