.parentOp-dom~button.btn-menu.btn.active .btn__content:before
  background:transparent

.activekey .parentOp-dom.btn-menu.btn .btn__content:before
  background:transparent

.activekey .parentOp.btn-menu.btn .btn__content:before
  background:c-text-title-pink

